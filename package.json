{
  "name": "WalkWithMe",
  "version": "1.0.1",
  "private": true,
  "repository": "https://github.com/chapati23/walk-with-me",
  "license": "MIT",
  "engines": {
    "node": ">=5.3.0"
  },
  "scripts": {
    "autoprefixer": "postcss -r --use autoprefixer -b 'last 2 versions' dist/styles/index.css",
    "build": "npm run clean && npm run js:lint -s && npm run js:inject:build -s && npm run js:bundle -s && npm run copy:html -s && npm run styles:build -s && npm run imagemin -s && npm run copy:assets -s",
    "clean": "rimraf .tmp/* dist/* && echo 'Cleaned up .tmp/ and dist/'",
    "copy:assets": "node scripts/copy.js --html",
    "copy:html": "node scripts/copy.js --assets",
    "predeploy": "npm run build -s",
    "deploy": "ghpages dist --domain=www.walkwithme.berlin",
    "imagemin": "node scripts/imagemin.js",
    "js:bundle": "jspm bundle-sfx src/app.js dist/app.bundle.js --minify",
    "js:inject:dev": "node scripts/js-inject.js --env=dev",
    "js:inject:build": "node scripts/js-inject.js --env=build",
    "js:lint": "eslint src/**/*.js !src/jspm_packages/**/*.js",
    "preserve:dev": "npm run styles:dev -s && npm run js:inject:dev -s",
    "serve:dev": "node scripts/server.js --env=dev",
    "preserve:build": "npm run build -s",
    "serve:build": "node scripts/server.js --env=build",
    "prestart": "npm run clean -s",
    "start": "npm run serve:dev -s",
    "styles:dev": "node-sass --source-map=true src/styles/index.scss .tmp/styles/index.css",
    "styles:build": "node-sass --source-map=true src/styles/index.scss dist/styles/index.css",
    "poststyles:build": "npm run autoprefixer -s",
    "test": "karma start --single-run"
  },
  "devDependencies": {
    "autoprefixer": "~6.2.1",
    "babel-core": "^5.8.34",
    "browser-sync": "^2.2.1",
    "es6-module-loader": "^0.17.8",
    "eslint": "~1.10.3",
    "ghpages": "0.0.3",
    "gulp": "^3.9.0",
    "gulp-gh-pages-cname": "^0.5.2",
    "gulp-load-plugins": "^0.10.0",
    "gulp-shell": "~0.5.1",
    "imagemin": "~4.0.0",
    "jspm": "^0.16.19",
    "jspm-bower-endpoint": "^0.3.2",
    "jspm-github": "^0.13.9",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.1",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-jspm": "^2.0.2",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-spec-reporter": "0.0.22",
    "karma-systemjs": "^0.9.0",
    "node-sass": "~3.4.2",
    "opn": "^1.0.1",
    "phantomjs": "^1.9.18",
    "postcss-cli": "~2.3.2",
    "recursive-copy": "~1.0.10",
    "rimraf": "latest",
    "shelljs": "~0.5.3",
    "systemjs": "^0.19.9",
    "systemjs-builder": "^0.14.15",
    "yargs": "~3.31.0"
  },
  "jspm": {
    "dependencies": {
      "angular": "github:angular/bower-angular@1.5.0-rc.0",
      "angular-animate": "github:angular/bower-angular-animate@1.5.0-rc.0",
      "angular-elastic": "npm:angular-elastic@^2.5.0",
      "angular-inview": "npm:angular-inview@^1.5.0",
      "angular-messages": "github:angular/bower-angular-messages@^1.5.0-rc.0",
      "angular-mocks": "github:angular/bower-angular-mocks@^1.5.0-rc.0",
      "angular-sanitize": "github:angular/bower-angular-sanitize@^1.5.0-rc.0",
      "angular-ui-router": "github:angular-ui/ui-router@^0.2.15",
      "angularfire": "npm:angularfire@^1.1.3",
      "api-check": "npm:api-check@^7.5.3",
      "connect-modrewrite": "npm:connect-modrewrite@^0.8.2",
      "firebase": "github:firebase/firebase-bower@^2.3.1",
      "jssha": "github:Caligatio/jsSHA@^2.0.1",
      "ng-file-upload": "npm:ng-file-upload@^10.0.2",
      "odometer": "bower:odometer@^0.4.7"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    },
    "overrides": {
      "github:angular-ui/ui-router@0.2.15": {
        "registry": "jspm",
        "main": "angular-ui-router",
        "dependencies": {
          "angular": "1.5.0-rc.0"
        }
      },
      "github:angular/bower-angular-animate@1.5.0-rc.0": {
        "shim": {
          "angular-animate": {
            "deps": [
              "angular"
            ]
          }
        }
      }
    }
  }
}
